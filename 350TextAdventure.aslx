    <object name="Terminal">
      <inherit name="editor_object" />
      <displayverbs type="stringlist">
        <value>Look at</value>
        <value>Use</value>
      </displayverbs>
      <feature_usegive />
      <attr name="feature_switchable" type="boolean">false</attr>
      <use type="script"><![CDATA[
        if (IsSwitchedOn(Terminal)) {
          msg ("Please enter the password:<br/>")
          get input {
            if (result = "password") {
              msg ("Password Correct. <br/><br/>Droid initiated in Security Room<br/>Solar Array System Initiated. Power has been stored.<br/><br/>Access to Security now GRANTED")
            }
            UnlockExit (Exit Start to security)
            UnlockExit (Exit Solar to security)
            UnlockExit (Exit Biosphere to Security)
            UnlockExit (Exit Armory to Security)
            UnlockExit (Exit Med Bay to Security)
            UnlockExit (Exit Living Quarters to Security)
          }
        }
      ]]></use>
    </object>
